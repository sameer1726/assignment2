nday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\",\n      };\n      const cd = new Date(thisDate.split());\n      let day = cd.getDay();\n      this.current_day = days[day]; //day\n      let dateArr = thisDate.split(\"-\"); //date\n      this.current_date = dateArr[2] + \"th\";\n      const current_day_date_display =\n        this.current_day.substring(0, 3) + \" \" + this.current_date;\n      return current_day_date_display;\n    },\n\n    callingHourCard() {\n      console.log(this.current_date);\n      EventBus.$emit(\"curr_date\", this.current_date);\n    },\n\n    // iconQueryMaker(weatherType) {\n    //   let temp = weatherType.slice().toLowerCase();\n    //   temp = temp.replaceAll(\" \", \"_\");\n    //   console.log(\"temp: \", temp);\n    //   console.log(\"icon maker: \", \"@/assets/images/\" + this.icon.temp);\n    //   return \"@/assets/images/\" + temp;\n    // },\n  },\n};\n</script>\n\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.week-cards {\n  /* padding-top: 1%; */\n  display: flex;\n  justify-content: center;\n  width: 100vw;\n  height: 28.5vh;\n  padding-right: 0.2%;\n  padding-left: 0.2%;\n}\n\n.week-card {\n  min-width: 8vw;\n  height: 25.5vh;\n  padding: 1%;\n  margin: 0.2%;\n  margin-right: 1.2%;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n\n  background: rgba(255, 255, 255, 0.04);\n  border: 1px solid rgba(0, 0, 0, 0.16);\n  border-radius: 20px;\n}\n\n.week-card:hover {\n  box-shadow: 0px 4px 3px rgba(0, 0, 0, 0.45);\n  cursor: pointer;\n}\n\n.image {\n  width: 100%;\n  height: 14vh;\n  flex: none;\n  order: 0;\n  align-self: stretch;\n  flex-grow: 1;\n}\n\n.image img {\n  height: 12vh;\n  object-fit: cover;\n}\n\n/* .temp .header {\n  font-size: 15px;\n  opacity: 0.8;\n} */\n\n.temp .data {\n  font-size: 19px;\n  font-weight: 550;\n  letter-spacing: 0.05em;\n}\n\n.temp sup {\n  font-size: 10.5px;\n  font-weight: 500;\n  opacity: 0.8;\n}\n\n.date p {\n  font-size: 15px;\n  opacity: 0.6;\n}\n\n.type {\n  padding-top: 3%;\n  opacity: 0.6;\n  font-size: 14px;\n  text-transform: capitalize;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}