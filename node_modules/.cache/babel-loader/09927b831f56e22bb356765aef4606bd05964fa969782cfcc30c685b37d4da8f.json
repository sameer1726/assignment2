{"ast":null,"code":"// import EventBus from \"../main.js\";\n\nexport default {\n  data() {\n    return {\n      date: \"\",\n      hours: undefined\n    };\n  },\n  methods: {\n    days_details(details) {\n      this.date = details.date;\n      this.hours = details.hour;\n    },\n    manageTime(dateTime) {\n      return dateTime.split(\" \")[1];\n    },\n    manageDate(localDate) {\n      const months = {\n        \"01\": \"Jan\",\n        \"02\": \"Feb\",\n        \"03\": \"Mar\",\n        \"04\": \"Apr\",\n        \"05\": \"May\",\n        \"06\": \"Jun\",\n        \"07\": \"Jul\",\n        \"08\": \"Aug\",\n        \"09\": \"Sep\",\n        10: \"Oct\",\n        11: \"Nov\",\n        12: \"Dec\"\n      };\n      const days = {\n        0: \"Sunday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\"\n      };\n      const cd = new Date(localDate.split());\n      let day = cd.getDay();\n      let current_day = days[day]; //day\n      let dateArr = localDate.split(\"-\"); //date\n      let current_month = months(dateArr[1]);\n      let current_date = dateArr[2];\n      return current_day + \",\" + current_date + \"th \";\n    }\n  },\n  created() {\n    // Sets up the Event Bus listener using\n    // the custom event name and assosciates\n    // it with a component method.\n    this.$root.$on(\"days_details\", this.days_details);\n  },\n  destroyed() {\n    // Removes Event Bus listener upon removal\n    // of template from DOM.\n    this.$root.$off(\"days_details\", this.days_details);\n  }\n};","map":{"version":3,"mappings":"AAgCA;;AAEA;EACAA;IACA;MACAC;MACAC;IACA;EACA;EAEAC;IACAC;MACA;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;MACA;MAEA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;MACA;MAEA;MACA;MACA;MACA;MACA;MACA;MACA;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;EACA;EACAC;IACA;IACA;IACA;EACA;AACA","names":["data","date","hours","methods","days_details","manageTime","manageDate","created","destroyed"],"sourceRoot":"src/components","sources":["HourCard.vue"],"sourcesContent":["<template>\n  <div class=\"hour-cards\">\n    <div v-for=\"(hour, index) in hours\" :key=\"index\" class=\"hour-card\">\n      <div class=\"date\">\n        <h3>{{ manageDate(date) }}</h3>\n      </div>\n      <div class=\"time\">\n        <p>{{ manageTime(hour.time) }}</p>\n      </div>\n      <div class=\"bottom\">\n        <div class=\"temp\">\n          <div class=\"f\">\n            <p>{{ hour.temp_f }}ºF</p>\n          </div>\n          <div class=\"c\">\n            <p>{{ hour.temp_c }}ºC</p>\n          </div>\n        </div>\n        <div class=\"details\">\n          <div class=\"humidity\">\n            <p>Humidity: {{ hour.humidity }}</p>\n          </div>\n          <div class=\"visibility\">\n            <p>Visibility: {{ hour.vis_km }}km</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// import EventBus from \"../main.js\";\n\nexport default {\n  data() {\n    return {\n      date: \"\",\n      hours: undefined,\n    };\n  },\n\n  methods: {\n    days_details(details) {\n      this.date = details.date;\n      this.hours = details.hour;\n    },\n\n    manageTime(dateTime) {\n      return dateTime.split(\" \")[1];\n    },\n\n    manageDate(localDate) {\n      const months = {\n        \"01\": \"Jan\",\n        \"02\": \"Feb\",\n        \"03\": \"Mar\",\n        \"04\": \"Apr\",\n        \"05\": \"May\",\n        \"06\": \"Jun\",\n        \"07\": \"Jul\",\n        \"08\": \"Aug\",\n        \"09\": \"Sep\",\n        10: \"Oct\",\n        11: \"Nov\",\n        12: \"Dec\",\n      };\n\n      const days = {\n        0: \"Sunday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\",\n      };\n\n      const cd = new Date(localDate.split());\n      let day = cd.getDay();\n      let current_day = days[day]; //day\n      let dateArr = localDate.split(\"-\"); //date\n      let current_month = months(dateArr[1]);\n      let current_date = dateArr[2];\n      return current_day + \",\" + current_date + \"th \"\n    },\n  },\n  created() {\n    // Sets up the Event Bus listener using\n    // the custom event name and assosciates\n    // it with a component method.\n    this.$root.$on(\"days_details\", this.days_details);\n  },\n  destroyed() {\n    // Removes Event Bus listener upon removal\n    // of template from DOM.\n    this.$root.$off(\"days_details\", this.days_details);\n  },\n};\n</script>\n\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.hour-cards {\n  display: flex;\n  overflow-x: scroll;\n  padding-bottom: 0.5%;\n  width: 100vw;\n  height: 28vh;\n  padding-right: 0.2%;\n  padding-left: 0.2%;\n}\n\n.hour-card {\n  /* width: 30vw; */\n  min-width: 17vw;\n  padding: 1%;\n  margin: 0.2%;\n  height: 26vh;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  /* align-items: center; */\n\n  background: rgba(255, 255, 255, 0.25);\n  border: 2px solid rgba(0, 0, 0, 0.16);\n  border-radius: 10px;\n}\n\n.date h3 {\n  /* width: 14vw; */\n  text-align: left;\n  font-size: 21px;\n}\n\n.time p {\n  letter-spacing: 1em;\n  text-align: center;\n  font-size: 17px;\n  font-weight: 600;\n\n  flex: none;\n  order: 1;\n  align-self: stretch;\n  flex-grow: 0;\n}\n\n.temp {\n  /* margin: 3%; */\n  display: flex;\n  justify-content: space-between;\n  margin-right: 12%;\n  margin-left: 12%;\n}\n\n.f p,\n.c p {\n  font-size: 17px;\n}\n\n.details {\n  margin-top: 4%;\n  display: flex;\n  justify-content: space-between;\n}\n\n.humidity p,\n.visibility p {\n  font-size: 15px;\n  opacity: 0.8;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}