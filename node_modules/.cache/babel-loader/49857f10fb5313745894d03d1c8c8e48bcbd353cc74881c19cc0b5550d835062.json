{"ast":null,"code":"import { mapActions, mapGetters } from \"vuex\";\nexport default {\n  data() {\n    return {\n      current_date: \"\",\n      current_day: \"\",\n      icon: {\n        sunny: \"sunny-day.png\",\n        clear: \"sunny-day.png\",\n        cloudy: \"cloudy.png\",\n        partly_cloudy: \"cloudy.png\",\n        rain: \"rainy.png\",\n        moderate_rain: \"rainy.png\",\n        patchy_rain_possible: \"rainy.png\"\n      }\n    };\n  },\n  computed: {\n    ...mapGetters([\"getWeather\"])\n  },\n  methods: {\n    ...mapActions([\"GET_WEATHER\"]),\n    dateTimeManager(thisDate) {\n      console.log(\"thisdate: \" + thisDate);\n      const days = {\n        0: \"Sunday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\"\n      };\n      const cd = new Date(thisDate);\n      let day = cd.getDay();\n      this.current_day = days[day]; //day\n      let dateArr = thisDate.split(\"-\"); //date\n      this.current_date = dateArr[2] + \"th\";\n      const current_day_date_display = this.current_day.substring(0, 3) + \" \" + this.current_date;\n      return current_day_date_display;\n    },\n    iconQueryMaker(weatherType) {\n      let temp = weatherType.toLowerCase();\n      temp = temp.replace(\" \", \"_\");\n      console.log(\"icon maker\");\n      return \"@/assets/images/\" + temp;\n    }\n  },\n  created: function () {\n    this.$store.dispatch(\"GET_WEATHER\");\n  }\n};","map":{"version":3,"mappings":"AA+BA;AAEA;EACAA;IACA;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;IACA;EACA;EAEAC;IACA;EACA;EAEAC;IACA;IAEAC;MACAC;MACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA,iCACA;MACA;IACA;IAEAC;MACA;MACAC;MACAF;MACA;IACA;EACA;EAEAG;IACA;EACA;AACA","names":["data","current_date","current_day","icon","sunny","clear","cloudy","partly_cloudy","rain","moderate_rain","patchy_rain_possible","computed","methods","dateTimeManager","console","iconQueryMaker","temp","created"],"sourceRoot":"src/components","sources":["WeekCard.vue"],"sourcesContent":["<template>\n  <div class=\"week-cards\">\n    <div\n      v-for=\"(thisDay, index) in getWeather.forecast.forecastday\"\n      :key=\"index\"\n      class=\"week-card\"\n    >\n      <div class=\"image\">\n        <!-- <img src=\"@/assets/images/sunny-day.png\" alt=\"loading..\" /> -->\n        <img\n          v-bind:src=\"iconQueryMaker(thisDay.day.condition.text)\"\n          alt=\"loading..\"\n        />\n      </div>\n      <div class=\"temp\">\n        <p class=\"data\">\n          {{ thisDay.day.maxtemp_f }}ยบ<sup>F</sup> โข\n          {{ thisDay.day.mintemp_f }}ยบ<sup>F</sup>\n        </p>\n      </div>\n      <div class=\"date\">\n        {{ dateTimeManager(thisDay.date) }}\n      </div>\n      <div class=\"type\">\n        {{ thisDay.day.condition.text }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      current_date: \"\",\n      current_day: \"\",\n      icon: {\n        sunny: \"sunny-day.png\",\n        clear: \"sunny-day.png\",\n        cloudy: \"cloudy.png\",\n        partly_cloudy: \"cloudy.png\",\n        rain: \"rainy.png\",\n        moderate_rain: \"rainy.png\",\n        patchy_rain_possible: \"rainy.png\",\n      },\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"getWeather\"]),\n  },\n\n  methods: {\n    ...mapActions([\"GET_WEATHER\"]),\n\n    dateTimeManager(thisDate) {\n      console.log(\"thisdate: \" + thisDate);\n      const days = {\n        0: \"Sunday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\",\n      };\n      const cd = new Date(thisDate);\n      let day = cd.getDay();\n      this.current_day = days[day]; //day\n      let dateArr = thisDate.split(\"-\"); //date\n      this.current_date = dateArr[2] + \"th\";\n      const current_day_date_display =\n        this.current_day.substring(0, 3) + \" \" + this.current_date;\n      return current_day_date_display;\n    },\n\n    iconQueryMaker(weatherType) {\n      let temp = weatherType.toLowerCase();\n      temp = temp.replace(\" \", \"_\");\n      console.log(\"icon maker\")\n      return \"@/assets/images/\" + temp;\n    },\n  },\n\n  created: function () {\n    this.$store.dispatch(\"GET_WEATHER\");\n  },\n};\n</script>\n\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.week-cards {\n  /* padding-top: 1%; */\n  display: flex;\n  justify-content: center;\n  width: 100vw;\n  height: 28.5vh;\n  padding-right: 0.2%;\n  padding-left: 0.2%;\n}\n\n.week-card {\n  min-width: 8vw;\n  height: 25.5vh;\n  padding: 1%;\n  margin: 0.2%;\n  margin-right: 1.2%;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n\n  background: rgba(255, 255, 255, 0.04);\n  border: 1px solid rgba(0, 0, 0, 0.16);\n  border-radius: 20px;\n}\n\n.week-card:hover {\n  box-shadow: 0px 4px 3px rgba(0, 0, 0, 0.45);\n  cursor: pointer;\n}\n\n.image {\n  width: 100%;\n  height: 14vh;\n  flex: none;\n  order: 0;\n  align-self: stretch;\n  flex-grow: 1;\n}\n\n.image img {\n  height: 12vh;\n  object-fit: cover;\n}\n\n/* .temp .header {\n  font-size: 15px;\n  opacity: 0.8;\n} */\n\n.temp .data {\n  font-size: 19px;\n  font-weight: 550;\n  letter-spacing: 0.05em;\n}\n\n.temp sup {\n  font-size: 10.5px;\n  font-weight: 500;\n  opacity: 0.8;\n}\n\n.date p {\n  font-size: 15px;\n  opacity: 0.6;\n}\n\n.type {\n  padding-top: 3%;\n  opacity: 0.6;\n  font-size: 14px;\n  text-transform: capitalize;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}