</div>\n      <div class=\"date\">\n        <p>{{ current_date }}</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from \"vuex\";\n\nexport default {\n  data() {\n    return {\n      current_date: \"\",\n      current_day: \"\",\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"getWeather\"]),\n  },\n\n  methods: {\n    ...mapActions([\"GET_WEATHER\"]),\n\n    dateTimeManager(thisDate) {\n      // const dateTime = this.getWeather.location.localtime;\n      const days = {\n        0: \"Sunday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\",\n      };\n      const cd = new Date(thisDate);\n      let day = cd.getDay();\n\n      this.current_day = days[day]; //day\n\n      let datetime = thisDate.split(\" \"); //date\n      let dateArr = datetime[0].split(\"-\");\n      this.current_date =\n        dateArr[2] + \".\" + dateArr[1] + \".\" + dateArr[0].substring(2);\n    },\n  },\n\n  created: function () {\n    this.$store.dispatch(\"GET_WEATHER\");\n  },\n};\n</script>\n\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.week-cards {\n  /* padding-top: 1%; */\n  display: flex;\n  justify-content: center;\n  width: 100vw;\n  height: 28.5vh;\n  padding-right: 0.2%;\n  padding-left: 0.2%;\n}\n\n.week-card {\n  min-width: 8vw;\n  height: 22vh;\n  padding: 1%;\n  margin: 0.2%;\n  margin-right: 1.2%;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n\n  background: rgba(255, 255, 255, 0.04);\n  border: 1px solid rgba(0, 0, 0, 0.16);\n  border-radius: 20px;\n}\n\n.week-card:hover {\n  box-shadow: 0px 4px 3px rgba(0, 0, 0, 0.45);\n  cursor: pointer;\n}\n\n.image {\n  width: 7vw;\n  height: 14vh;\n  flex: none;\n  order: 0;\n  align-self: stretch;\n  flex-grow: 1;\n}\n\n.image img {\n  width: 7vw;\n  height: 12vh;\n  object-fit: cover;\n}\n\n.temp p {\n  font-size: 19px;\n  font-weight: 550;\n  letter-spacing: 0.05em;\n}\n\n.temp sup {\n  font-size: 11px;\n  font-weight: 500;\n  opacity: 0.8;\n}\n\n.date p {\n  font-size: 15px;\n  opacity: 0.6;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}