{"ast":null,"code":"import HourCard from \"@/components/HourCard.vue\";\nimport WeekCard from \"@/components/WeekCard.vue\";\nimport { mapActions, mapGetters } from \"vuex\";\nexport default {\n  components: {\n    HourCard,\n    WeekCard\n  },\n  data() {\n    return {\n      query: \"\",\n      ref_temp: true,\n      current_day: \"\",\n      current_date: \"\",\n      current_time: \"\"\n    };\n  },\n  computed: {\n    ...mapGetters([\"getWeather\"])\n  },\n  methods: {\n    ...mapActions([\"GET_WEATHER\"]),\n    getF() {\n      this.ref_temp = true;\n    },\n    getC() {\n      this.ref_temp = false;\n    },\n    dateTimeManager() {\n      const dateTime = this.getWeather.location.localtime;\n      const days = {\n        0: \"Sunday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\"\n      };\n      const cd = new Date(dateTime);\n      let day = cd.getDay();\n      this.current_day = days[day]; //day\n\n      let datetime = this.getWeather.location.localtime.split(\" \"); //date\n      let dateArr = datetime[0].split(\"-\");\n      this.current_date = dateArr[2] + \".\" + dateArr[1] + \".\" + dateArr[0].substring(2);\n      this.current_time = datetime[1]; //time\n    }\n  },\n\n  created: function () {\n    this.$store.dispatch(\"GET_WEATHER\");\n  }\n};","map":{"version":3,"mappings":"AAsEA;AACA;AACA;AAEA;EACAA;IACAC;IACAC;EACA;EAEAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EAEAC;IACA;EACA;EAEAC;IACA;IAEAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;MACA;MACA;MACA;MAEA;;MAEA;MACA;MACA,oBACAC;MAEA;IACA;EACA;;EAEAC;IACA;EACA;AACA","names":["components","HourCard","WeekCard","data","query","ref_temp","current_day","current_date","current_time","computed","methods","getF","getC","dateTimeManager","dateArr","created"],"sourceRoot":"src/views","sources":["HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"home-page\">\n    <div class=\"top-bar\">\n      <div class=\"brand-name\">\n        <p>WeatherSure</p>\n      </div>\n      <div class=\"search-bar\">\n        <!-- <img src=\"@/assets/images/search-icon.png\" /> -->\n        <input\n          type=\"search\"\n          name=\"search-bar\"\n          class=\"s-box\"\n          placeholder=\"Search for Location\"\n          v-model=\"query\"\n        />\n      </div>\n    </div>\n    <div class=\"mid-bar\">\n      <div class=\"date\">\n        {{ dateTimeManager() }}\n        <div class=\"date-time\">\n          <p>{{ this.current_time }}</p>\n        </div>\n        <div class=\"date-day\">\n          <p>{{ this.current_day }} {{ this.current_date }}</p>\n        </div>\n      </div>\n      <div class=\"temp\">\n        <div class=\"location\">\n          <p>\n            {{ getWeather.location.name }}, {{ getWeather.location.region }}\n          </p>\n        </div>\n        <div class=\"t\">\n          <div class=\"number\">\n            <h2 v-if=\"ref_temp\">{{ getWeather.current.temp_f }}º</h2>\n            <h2 v-if=\"!ref_temp\">{{ getWeather.current.temp_c }}º</h2>\n          </div>\n          <div class=\"f-c\">\n            <p @click=\"getF\">F</p>\n            <p @click=\"getC\">C</p>\n          </div>\n        </div>\n        <div class=\"type\">\n          <p>{{ getWeather.current.condition.text }}</p>\n        </div>\n      </div>\n      <div class=\"details\">\n        <p>\n          Feels Like : {{ getWeather.current.feelslike_f }}ºF |\n          {{ getWeather.current.feelslike_c }}ºC\n        </p>\n        <p>Humidity : {{ getWeather.current.humidity }}</p>\n        <p>Cloud : {{ getWeather.current.cloud }}</p>\n        <p>Wind Speed : {{ getWeather.current.wind_mph }}mph</p>\n        <p>Visibility : {{ getWeather.current.vis_km }}km</p>\n      </div>\n    </div>\n    <div class=\"week-title\">\n      <p>week</p>\n    </div>\n    <week-card></week-card>\n    <div class=\"hour-title\">\n      <p>2 hourly</p>\n    </div>\n    <hour-card></hour-card>\n  </div>\n</template>\n\n<script>\nimport HourCard from \"@/components/HourCard.vue\";\nimport WeekCard from \"@/components/WeekCard.vue\";\nimport { mapActions, mapGetters } from \"vuex\";\n\nexport default {\n  components: {\n    HourCard,\n    WeekCard,\n  },\n\n  data() {\n    return {\n      query: \"\",\n      ref_temp: true,\n      current_day: \"\",\n      current_date: \"\",\n      current_time: \"\",\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"getWeather\"]),\n  },\n\n  methods: {\n    ...mapActions([\"GET_WEATHER\"]),\n\n    getF() {\n      this.ref_temp = true;\n    },\n    getC() {\n      this.ref_temp = false;\n    },\n    dateTimeManager() {\n      const dateTime = this.getWeather.location.localtime;\n      const days = {\n        0: \"Sunday\",\n        1: \"Monday\",\n        2: \"Tuesday\",\n        3: \"Wednesday\",\n        4: \"Thursday\",\n        5: \"Friday\",\n        6: \"Saturday\",\n      };\n      const cd = new Date(dateTime);\n      let day = cd.getDay();\n\n      this.current_day = days[day]; //day\n\n      let datetime = this.getWeather.location.localtime.split(\" \"); //date\n      let dateArr = datetime[0].split(\"-\");\n      this.current_date =\n        dateArr[2] + \".\" + dateArr[1] + \".\" + dateArr[0].substring(2);\n\n      this.current_time = datetime[1]; //time\n    },\n  },\n\n  created: function () {\n    this.$store.dispatch(\"GET_WEATHER\");\n  },\n};\n</script>\n\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.top-bar {\n  height: 8vh;\n  width: 100vw;\n  padding: 1.5%;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n\n.brand-name p {\n  font-style: normal;\n  font-weight: 700;\n  font-size: 36px;\n\n  letter-spacing: 0.015em;\n  background: linear-gradient(\n    180deg,\n    #d8e3ff 50%,\n    rgba(216, 227, 255, 0) 85.42%\n  );\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent;\n  background-clip: text;\n  backdrop-filter: blur(2px);\n}\n\n.search-bar {\n  mix-blend-mode: luminosity;\n}\n\n.s-box {\n  padding: 2%;\n  background: rgba(255, 255, 255, 0.16);\n  border-radius: 50px;\n  border: 2px solid rgba(255, 255, 255, 0);\n  width: 28vw;\n  height: 4vh;\n}\n\n/* .s-box :focus {\n  color: white;\n} */\n\n::placeholder {\n  font-size: 13px;\n  text-align: center;\n  color: white;\n}\n\n.mid-bar {\n  height: 22vh;\n  width: 100vw;\n  margin-top: 2.5%;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n\n.date {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-content: center;\n  margin-left: 3%;\n  margin-bottom: 11%;\n  gap: 4px;\n}\n\n.date-time p {\n  font-size: 35px;\n  font-weight: 400;\n  letter-spacing: 0.3em;\n}\n\n.date-day p {\n  font-weight: 800;\n  font-size: 19px;\n}\n\n.temp {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  margin-left: 10%;\n  margin-bottom: 3.5%;\n}\n\n.location {\n  font-weight: 100;\n  font-size: 24px;\n}\n\n.t {\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  gap: 20px;\n}\n\n.number h2 {\n  font-size: 62px;\n}\n\n.f-c {\n  transition: 1s;\n}\n\n.f-c p:hover {\n  cursor: pointer;\n  font-size: 20px;\n}\n\n.f-c p {\n  padding: 30%;\n}\n\n.type {\n  font-size: 19px;\n  font-weight: 500;\n}\n\n.details {\n  padding: 0.4%;\n  background: rgba(255, 255, 255, 0.12);\n  border-radius: 10px;\n  margin-right: 3.8%;\n  margin-bottom: 3.2%;\n\n  display: flex;\n  flex-direction: column;\n  width: 23vw;\n  height: 20vh;\n\n  font-weight: 400;\n  font-size: 18px;\n  line-height: 170%;\n\n  text-align: center;\n}\n\n.details p {\n  font-size: 16px;\n  font-weight: 400;\n  /* opacity: 0.8; */\n  letter-spacing: 0.06em;\n}\n\n.week-title {\n  text-align: left;\n  margin-left: 8%;\n}\n\n.hour-title {\n  text-align: left;\n  margin-left: 1%;\n}\n\n.week-title p,\n.hour-title p {\n  font-weight: 700;\n  font-size: 28px;\n  text-transform: uppercase;\n  font-style: normal;\n  color: rgba(255, 255, 255, 0.6);\n  flex: none;\n  order: 0;\n  flex-grow: 1;\n  opacity: 0.9;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}